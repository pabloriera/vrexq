do
    hush
    let ns p = n (scale "majPent" p)
        modP = pF "modP"
        carP = pF "carP"
        index = pF "index"
    d1 
        $ slow 1
        $ every 4 (slow 1.5) 
        $ stack [
            n "[0 <~ 2 3>]" 
            # s "industrial" 
            # end 0.01 
            # gain (rand*0.1 + 0.7),
            s "bassdm*2"
            # gain 0.7
            ]
        # orbit 2 
        # room 0.1 
        # size 0.2
    d4 
        $ slow 0.5 
        $ ns "<0 0*2 -2*2 <[0 -2] [0 -2 2 4]>>" 
        # s "supersquare"
        # attack 0.01 # release 0.08
        # sustain 0.1
        # shape 0.2
        # octave 3 
        # lpf 500 
        # lpq 0.1 
        # gain (rand*0.1 + 0.9)
    -- 
    d2 
        $ within (0.25, 0.75)(# lpf 200)
        $ fast 2
        $ every 2 (within (0, 0.5)(# nudge 0.0125) . (fast 2))
        $ cat [
        ns "[2 0 1 3]",
        ns "~ [0,2] ~  [3,5]",
        ns "2 ~ 1 ~",
        ns "3",
        ns "[0,2] ~  ~ [3,5]",
        ns "3 ~ ~ ~",
        ns "2 ~ 2 ~ ",
        ns "1 ~ ~ ~ "
        ]
        # s "supernoise"
        # begin (range 0.0125 0.5 sine)
        # gain 0
        # lpf (slow 2 $ range 200 10000 sine)
    d5 
        $ sometimesBy 0.25 (fast 2)
        $ every 3 (stut 3 0.2 (1/3) . (#speed 1.5))
        $ every 2 (within (0.5, 1) (stut 3 0.5 (1/3)).(fast 2))
        $ sometimesBy 0.25 (# speed 2)
        $ degradeBy 0.25
        $ s "<click click:1>*<4 8 16>"
        # hpf (slow 2 $ range 200 8000 sine)
        # hpq 0.3
        # gain 0.7
        # pan sine
    --
    d3
        $ slow 8
        $ ns "[0 4 3 -2, -4 -3]"
        # s "fms"
        # attack 8
        # sustain 10
        # release 8
        # detune "<2 3>"
        # modP "2.5 5"
        # index 2.01
        # gain 0.9
    --
    d6
        $ every 16 (within (0.5, 1) $ (# accelerate "-1"))
        $ every 3 (within (0.25, 0.75) $ jux rev)
        $ every 2 (within (0.5, 1) $ stut 4 0.125 (-1/4))
        $ every 4 (within (0.75, 1) $ rotL 0.125)
        $ slow 2
        $ ns "[0 2 5 4, 1(3,8), 2(6,16,2), 3(3,16,4), <4 5 6 7>*2]"
        # s "fms"
        # gain 0.87
        # modP "[10 20 15 20]*2"
        # sustain "[1.75 4]*8"
        # carP "[1 3 2 4]*2"
        # index "1 10.02 1 20.01"
        # pan (range 0.2 0.8 $ sine)
        # orbit "[3 4]*8"
    --
    d7
        $ every 2 (rotL 0.125)
        $ every 16 (# accelerate "-1")
        $ every 3 (within (0.75, 1) (# coarse 4))
        $ every 2 (within (0.5, 1) $ juxBy 0.25 rev . rotL 0.125)
        $ n "[3*2 ~ ~ ~, ~ [~ 4] [5 1*2] ~, 1*8]"
        # s "at2"
        # gain "[0.8 0.5]*4"

